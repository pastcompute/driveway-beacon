{
    "configurations": [
        {
            "name": "Arduino-teensy",
            "includePath": [
                "~/.platformio/packages/toolchain-gccarmnoneeabi/arm-none-eabi/include/**",
                "~/.platformio/packages/toolchain-gccarmnoneeabi/lib/gcc/arm-none-eabi/**",
                "~/.platformio/packages/framework-arduinoteensy/cores/**",
                "~/.platformio/packages/framework-arduinoteensy/libraries/**",
                "${workspaceFolder}/lib/**",
                "${workspaceFolder}/.pio/libdeps/**"
            ],
            "defines": [
                "ARDUINO=100",
                "TEENSYDUINO",
                "KINETISL",
                "__MK20DX256__",
                "Serial=Serial1",
                "F_CPU=48000000"
            ],
            "compilerPath": "~/.platformio/packages/toolchain-gccarmnoneeabi/bin/arm-none-eabi-gcc",
            "cStandard": "c99",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "linux-gcc-arm"
        },
        {
            "name": "Arduino-xmc",
            "forcedInclude": [
                "~/.platformio/packages/framework-arduino-avr/variants/XMC1100/config/XMC1100_XMC2GO/pins_arduino.h"
            ],
            "includePath": [
                "~/.platformio/packages/framework-arduinoxmc/cores/**",
                "~/.platformio/packages/framework-arduinoxmc/variants/XMC1100/**",
                "~/.platformio/packages/toolchain-gccarmnoneeabi/arm-none-eabi/include/**",
                "~/.platformio/packages/toolchain-gccarmnoneeabi/lib/gcc/arm-none-eabi/**",
                "~/.platformio/packages/framework-arduinoxmc/libraries/**",
                "${workspaceFolder}/lib/**",
                "${workspaceFolder}/.pio/libdeps/**"
            ],
            "defines": [
                "ARDUINO=100",
                "__USES_INITFINI__",
                "__STATIC_INLINE=",
                "XMC1100_Q024x0064",
                "XMC1100_Boot_Kit",
                "XMC1100_T038x0064",
                "XMC_BOARD"
            ],
            "compilerPath": "~/.platformio/packages/toolchain-gccarmnoneeabi/bin/arm-none-eabi-gcc",
            "cStandard": "c99",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}